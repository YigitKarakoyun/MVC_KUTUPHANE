@using MVC_KUTUPHANE.Models.Entity
@model TBL_PERSONEL
@{
    ViewBag.Title = "Guncelle";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>PERSONEL GUNCELLEME</h2>
<hr />
@using (Html.BeginForm("Guncelle", "Personel", FormMethod.Post))
{
    <div class="form-group">
        @*@Html.LabelFor(k => k.ID)*@
        @Html.HiddenFor(k => k.ID, new { @class = "form-control" })
        <br />
        @Html.LabelFor(k => k.PERSONEL)
        @Html.TextBoxFor(k => k.PERSONEL, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <button class="btn btn-warning">Güncelle</button>
    </div>

}

